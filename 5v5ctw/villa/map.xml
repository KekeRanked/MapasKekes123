<map proto="1.4.2">
<name>Villa</name>
<version>1.3.3</version>
<objective>Capture the opponent's wool!</objective>
<time result="objectives" max-overtime="10m" overtime="10m">25m</time>
<authors>
    <author uuid="6bec5c3a-4934-4b19-9aa8-c7c569820f7d"/> <!-- Claf -->
</authors>
<teams>
    <team id="blue-team" color="blue" max="5" max-overfill="5">Blue</team>
    <team id="red-team" color="dark red" max="5" max-overfill="5">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true" material="iron sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="28" amount="40" material="arrow"/>
        <item slot="2" unbreakable="true" material="iron pickaxe">
            <enchantment>efficiency</enchantment>
        </item>
        <item slot="3" unbreakable="true" material="iron axe">
            <enchantment>efficiency</enchantment>
        </item>
        <item slot="4" unbreakable="true" material="iron spade"/>
        <item slot="5" amount="64" damage="5" material="wood"/>
        <item slot="32" amount="64" damage="5" material="wood"/>
        <item slot="6" amount="64" material="glass"/>
        <item slot="7" material="golden apple"/>
        <effect duration="4s" amplifier="5">damage resistance</effect>
    </kit>
    <kit id="blue-kit" parents="spawn-kit">
        <helmet unbreakable="true" color="334CB2" material="leather helmet"/>
        <chestplate unbreakable="true" color="334CB2" material="leather chestplate">
            <enchantment>projectile_protection</enchantment>
        </chestplate>
    </kit>
    <kit id="red-kit" parents="spawn-kit">
        <helmet unbreakable="true" color="993333" material="leather helmet"/>
        <chestplate unbreakable="true" color="993333" material="leather chestplate">
            <enchantment>projectile_protection</enchantment>
        </chestplate>
    </kit>
</kits>
<spawns>
    <default>
        <region yaw="180">
            <point>-100.5,13,-56.5</point>
        </region>
    </default>
    <spawn team="blue-team" kit="blue-kit">
        <region yaw="90">
            <point>-12.5,8,-84.5</point>
        </region>
    </spawn>
    <spawn team="red-team" kit="red-kit">
        <region yaw="-90">
            <point>-187.5,8,-84.5</point>
        </region>
    </spawn>
</spawns>
<filters>
    <not id="deny-red-woolroom">
        <any>
            <team id="only-red">red-team</team>
            <material>chest</material>
        </any>
    </not>
    <not id="deny-blue-woolroom">
        <any>
            <team id="only-blue">blue-team</team>
            <material>chest</material>
        </any>
    </not>
    <all id="only-iron-regen">
        <material id="only-iron">iron block</material>
        <cause>world</cause>
    </all>
    <not id="deny-void">
        <void/>
    </not>
</filters>
<regions>
    <cuboid id="orange-woolroom" min="-6,0,-117" max="-18,25,-105"/> <!-- red gets -->
    <cuboid id="orange-gearroom" min="-24,0,-64" max="-33,25,-52"/> <!-- red gets -->
    <cuboid id="purple-woolroom" min="-194,0,-105" max="-182,25,-117"/> <!-- blue gets -->
    <cuboid id="purple-gearroom" min="-176,0,-52" max="-167,25,-64"/> <!-- blue gets -->
    <union id="spawns">
        <cuboid id="blue-spawn" min="-8,0,-95" max="-34,25,-74"/>
        <cuboid id="red-spawn" min="-192,0,-74" max="-166,25,-95"/>
    </union>
    <union id="spawns-iron">
        <cuboid min="-188,6,-81" max="-183,2,-88"/>
        <cuboid min="-12,6,-81" max="-17,2,-88"/>
    </union>
    <negative id="void-regions">
        <union>
            <rectangle min="-166,-81" max="-157,-88"/>
            <rectangle min="-34,-81" max="-43,-88"/>
            <complement>
                <rectangle min="-146,-52" max="-54,-117"/>
                <rectangle min="-146,-81" max="-138,-88"/>
                <rectangle min="-122,-78" max="-78,-88"/>
                <rectangle min="-105,-102" max="-96,-117"/>
                <rectangle min="-54,-88" max="-62,-81"/>
            </complement>
        </union>
    </negative>
    <union id="beacons-glass">
        <block>-13,7,-112</block>
        <block>-28,14,-58</block>
        <block>-173,14,-58</block>
        <block>-188,7,-112</block>
    </union>
    <apply region="blue-spawn" enter="only-blue" message="You may not enter the opponent's spawn!"/>
    <apply region="red-spawn" enter="only-red" message="You may not enter the opponent's spawn!"/>
    <apply region="purple-woolroom" enter="only-blue" message="You may not enter your own wool room!"/>
    <apply region="orange-woolroom" enter="only-red" message="You may not enter your own wool room!"/>
    <apply region="purple-gearroom" enter="only-blue" message="You may not enter your own shrine!"/>
    <apply region="orange-gearroom" enter="only-red" message="You may not enter your own shrine!"/>
    <apply region="beacons-glass" block="never"/>
    <apply region="spawns" block-break="only-iron" block-place="only-iron-regen" message="You may only break iron in the spawn!"/>
    <apply region="orange-woolroom" block="deny-blue-woolroom" use="only-red"/>
    <apply region="purple-woolroom" block="deny-red-woolroom" use="only-blue"/>
    <apply region="orange-gearroom" block="deny-blue-woolroom" use="only-red"/>
    <apply region="purple-gearroom" block="deny-red-woolroom" use="only-blue"/>
    <apply region="void-regions" block-place="deny-void" message="You may not edit the void here!"/>
</regions>
<renewables>
    <renewable rate="1" grow="false" particles="true" sound="true" avoid-entities="true" region="spawns-iron" renew-filter="only-iron"/>
</renewables>
<wools wool-proximity-metric="none" monument-proximity-metric="closest block">
    <wool team="blue-team" color="purple" craftable="false" location="-190,8,-111">
        <monument>
            <block>-20,9,-85</block>
        </monument>
    </wool>
    <wool team="red-team" color="orange" craftable="false" location="-10,8,-111">
        <monument>
            <block>-181,9,-85</block>
        </monument>
    </wool>
</wools>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
    <tool>iron axe</tool>
    <tool>iron spade</tool>
</toolrepair>
<itemremove>
    <item>wood</item>
    <item>glass</item>
    <item>golden carrot</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>string</item>
    <item>seeds</item>
    <item>red rose</item>
    <item>yellow flower</item>
    <item>apple</item>
    <item>redstone</item>
</itemremove>
<itemkeep>
    <item>arrow</item>
</itemkeep>
<kill-reward>
    <item amount="32" damage="5" material="wood"/>
    <item amount="16" material="glass"/>
    <item material="golden apple"/>
    <item amount="10" material="arrow"/>
</kill-reward>
<crafting>
    <disable>iron trapdoor</disable>
    <disable>minecart</disable>
    <disable>boat</disable>
</crafting>
<item-mods>
    <rule>
        <match>
            <material>iron chestplate</material>
        </match>
        <modify>
            <enchantment>projectile_protection</enchantment>
        </modify>
    </rule>
</item-mods>
<maxbuildheight>21</maxbuildheight>
<hunger>
    <depletion>off</depletion>
</hunger>
<respawn delay="5s" auto="true"/>
</map>
